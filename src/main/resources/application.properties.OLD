quarkus.http.port=8080
quarkus.http.root-path=/api
quarkus.smallrye-openapi.path=/openapi
quarkus.swagger-ui.always-include=true
quarkus.swagger-ui.path=swagger-ui
quarkus.swagger-ui.theme=material
quarkus.oidc.auth-server-url=http://localhost:8081/auth/realms/daybird-quarkus
quarkus.oidc.client-id=daybird-app
quarkus.oidc.tls.verification=none
quarkus.oidc.authentication.user-info-required=true
quarkus.http.cors=true

quarkus.oidc-client.auth-server-url=http://localhost:8081/auth/realms/daybird-quarkus
quarkus.oidc-client.client-id=daybird-app
quarkus.oidc-client.grant.type=exchange

quarkus.oidc-token-propagation-reactive.exchange-token=true
quarkus.rest-client.database-api.url=http://localhost:8082/api
quarkus.rest-client.database-api.scope=javax.inject.Singleton
